<div class = "listing">
    <table>
	  <tr>
		
		<th>First Name</th>
		<th>Last Name</th>
		<th>Username</th>
		<th>User Level</th>
	  </tr>
	<% @users.each do |user| %>
		<tr class="<% cycle('odd', 'even') %>">
		  <td><%= user.first_name %></td>
		  <td><%= user.last_name %></td>
		  <td><%= user.username %></td>
		  <td><%= user.user_level %></td>
	      <td>
		<%= link_to("Edit", {:action => 'manage', :id => user}) %>
		<%#= link_to("Destroy", {:action => "manage", :id => user}, :confirm => "Are you sure you want to permanently remove this user?", :method => :post) %>
		</tr>
    <% end %>
    </table>
  </div>