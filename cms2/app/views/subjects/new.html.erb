<% @page_title = "Create Subject" %>

<%= link_to("<< Back to List", {:action => 'list', :subject_id => @subject_id}, :class => 'back-link') %>

<%= @subject_id %>


<div class="subject new">
  <h2>Create Subject</h2>

  <%= form_for(:subject, :url => {:action => 'create', :subject_id => @subject_id}) do |f| %>
  
    <%= render(:partial => "form", :locals => {:f => f}) %>
    
    <div class="form-buttons">
      <%= submit_tag("Create Subject") %>
    </div>
    
  <% end %>
</div>
