
<%= link_to("<< Back to List", {:action => 'list', :page_id => @page_id}, :class => 'back-link') %>

<%= @page_id.id %>

<div class="section new">
  <h2>Create Section</h2>

  <%= form_for(:section, :url => {:action => 'create', :page_id => @page_id}) do |f| %>
  
    <%= render(:partial => "form", :locals => {:f => f}) %>   
    <div class="form-buttons">
      <%= submit_tag("Create Section") %>
    </div>
    
  <% end %>
</div>
